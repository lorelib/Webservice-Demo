<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:jaxws="http://cxf.apache.org/jaxws"
       xsi:schemaLocation=" http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://cxf.apache.org/jaxws http://cxf.apache.org/schema/jaxws.xsd">

    <bean id="helloWorld" class="com.lorelib.webservice.cxf.client.HelloWorld"
          factory-bean="hwFactory" factory-method="create"/>

    <bean id="hwFactory" class="org.apache.cxf.jaxws.JaxWsProxyFactoryBean">
        <property name="serviceClass" value="com.lorelib.webservice.cxf.client.HelloWorld"/>
        <property name="address" value="http://localhost:8093/services/HelloWorld"/>
    </bean>
</beans>
